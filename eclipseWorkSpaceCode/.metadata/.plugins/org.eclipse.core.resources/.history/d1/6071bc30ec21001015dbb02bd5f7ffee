package com.orangeHr.qa.testcases;

import org.testng.annotations.Test;

public class Demotest {
	
	@Test
	public void testCase1() {
		System.out.println("Maven Test");
	}
	
	public void testCase2() {
		System.out.println("Maven Test2 from DemoTest class");
	}
}


//Class name should include 'test' word
// Method should have @Test annotation 
