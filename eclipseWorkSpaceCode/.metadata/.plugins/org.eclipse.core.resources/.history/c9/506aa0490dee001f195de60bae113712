package oopsConcepts.inheritance.Example;

public class CarExampleTestClass {

	public static void main(String[] args) {
		Car carObj = new Car();
		carObj.speed();
		carObj.engine();
		carObj.color();
		
		
		BMW bmwObj = new BMW();
		bmwObj.ventilatedSeats();  // BMW
		bmwObj.music(); // BMW
		bmwObj.speed(); // Car
		bmwObj.engine(); // Car
		bmwObj.color();  // Overridden method // considering the own BMW class
		
		
		BMWq7 bmwq7Obj = new bmwq7Obj;
		bmwq7Obj.engine(); //BMWq7  // Overridden method // considering the own BMWq7 class
		bmwq7Obj.color(); //BMW
		bmwq7Obj.ventilatedSeats();  //BMW
		bmwq7Obj.music(); //BMW
		bmwq7Obj.speed();  // Car

	}

}
