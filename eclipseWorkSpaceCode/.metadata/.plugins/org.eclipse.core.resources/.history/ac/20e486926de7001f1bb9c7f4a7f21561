package practiceAssignments;

public class OddNumbersAndMultiplicationTable {

	public static void main(String[] args) {
// 		1. Write a Java program to print odd numbers from 1 to 20 using a for loop.
		
		for(int i=1; i<20; i+=2) {
			System.out.println(i);			
		}
		
//		2. Write a Java program to print the multiplication table of 5 using a for loop
		
		int num = 5;
		for(int i=1; i<=10; i++) {
			int value = 5*i;
			System.out.println(num+" x "+i+" = "+value);	
		}
		
		
//		3. Star Pattern
//		
//		*****
//		****	
//		***	
//		**
//		*
		
		for(int i=5; i<=5; i++) {
			for(int j=5; j>i; j--) {
				System.out.println("*");
			}
		}


	}

}
